{
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"launch": { "configurations": [] },
	"security.workspace.trust.startupPrompt": "never",
	"redhat.telemetry.enabled": false,
	"settingsSync.ignoredExtensions": ["oderwat.indent-rainbow"],
	"svg.preview.mode": "svg",
	"telemetry.telemetryLevel": "off",
	// Terminal
	"terminal.integrated.cursorStyle": "underline",
	"terminal.integrated.fontFamily": "'Hack Nerd Font', monospace",
	// PLUGIN CONFIRM
	"git.confirmSync": false,
	"mdb.confirmRunAll": false,
	// JS
	"javascript.updateImportsOnFileMove.enabled": "always",
	"json.schemas": [],
	// VSCODE Window
	"window.restoreWindows": "none",
	// Explorer Settings
	"explorer.confirmDragAndDrop": false,
	"explorer.confirmDelete": false,
	// EDITOR WINDOW
	"editor.suggestSelection": "first",
	"editor.tabSize": 4,
	"[javascriptreact]": { "editor.tabSize": 2 },
	"[html]": { "editor.tabSize": 2 },
	"[django-html]": { "editor.tabSize": 2 },
	// ADD bracket Colors
	"editor.bracketPairColorization.enabled": true,
	"editor.guides.bracketPairs": "active",
	"python.formatting.provider": "autopep8",
	"[python]": { "editor.defaultFormatter": "ms-python.python" },
	// React Snippets
	"reactSnippets.settings.prettierEnabled": true,
	"reactSnippets.settings.importReactOnTop": false,
	// FILE OPEN ASSOCIATIONS
	"workbench.editorAssociations": {
		"*.db": "sqlite-viewer.option",
		"*.mongodb": "default"
	},
	"workbench.iconTheme": "material-icon-theme",
	// ICON FILE ASSOCIATIONS
	"material-icon-theme.files.associations": {
		"package-lock.json": "npm",
		// "package.json": "npm",
		"*.mongodb": "mint"
	},
	// ICON FOLDER ASSOCIATIONS
	"material-icon-theme.folders.associations": {
		// "partials": "components",
		"404": "error",
		"bootstrap": "css",
		"index": "home"
	},
	// FONT DECLARATIONS
	"editor.fontSize": 14,
	"editor.fontFamily": "'Fira Code', monospace",
	"editor.fontLigatures": true,
	"peacock.favoriteColors": [
		{
			"name": "Angular Red",
			"value": "#D80104"
		},
		{
			"name": "Azure Blue",
			"value": "#007fff"
		},
		{
			"name": "JavaScript Yellow",
			"value": "#f9e64f"
		},
		{
			"name": "Mandalorian Blue",
			"value": "#1857a4"
		},
		{
			"name": "Node Green",
			"value": "#215732"
