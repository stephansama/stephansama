{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"palette": {
		"os": "#ACB0BE",
		"red": "#ed8796",
		"pink": "#F5BDE6",
		"green": "#a6e3a1",
		"yellow": "#eed49f",
		"lavender": "#B7BDF8",
		"blue": "#89b4fa",
		"wight": "#FFFFFF",
		"text": "#24273a",
		"base": "#24273a"
	},
	"blocks": [
		{
			"alignment": "left",
			"type": "prompt",
			"newline": true,
			"segments": [
				{
					"type": "status",
					"style": "powerline",
					"foreground": "p:text",
					"background": "p:blue",
					"background_templates": [
						"{{ if .Error }}transparent{{ end }}"
					],
					"foreground_templates": [
						"{{ if .Error }}p:red{{ end }}"
					],
					"powerline_symbol": "▌",
					"leading_powerline_symbol": "█",
					"template": "{{ if .Error }} 󰋔{{ else }}{{ end }}",
					"properties": {
						"always_enabled": true
					}
				},
				{
					"background": "p:blue",
					"foreground": "p:text",
					"powerline_symbol": "\ue0b4",
					"style": "diamond",
					"template": "{{ .UserName }}",
					"type": "session"
				},
				{
					"background": "p:pink",
					"foreground": "p:text",
					"properties": {
						"folder_icon": "..\ue5fe..",
						"home_icon": "",
						"style": "agnoster_short",
						"mapped_locations": {
							"~": "",
							"~/.dotfiles/": " dotfiles",
							"~/.config": "󰒓 .config",
							"~/.config/nvim/": "",
							"~/Code/srandle-blog/": " blog"
						}
					},
					"powerline_symbol": "\ue0b4",
					"style": "powerline",
					"template": " {{ .Path }}",
					"type": "path"
				},
				{
					"background": "p:green",
					"foreground": "p:text",
					"style": "powerline",
					"properties": {
						"branch_icon": "\ue725 ",
						"cherry_pick_icon": "\ue29b ",
						"commit_icon": "\uf417 ",
						"fetch_status": false,
						"fetch_upstream_icon": false,
						"merge_icon": "\ue727 ",
						"no_commits_icon": "\uf0c3 ",
						"rebase_icon": "\ue728 ",
						"revert_icon": "\uf0e2 ",
						"tag_icon": "\uf412 "
					},
					"powerline_symbol": "\ue0b4",
					"template": " {{ .HEAD }}",
					"type": "git"
				}
			]
		},
		{
			"alignment": "left",
			"newline": true,
			"type": "prompt",
			"segments": [
				{
					"type": "time",
					"style": "plain",
					"properties": {
						"time_format": "15:04:05"
					}
				},
				{
					"type": "executiontime",
					"style": "powerline",
					"powerline_symbol": "",
					"foreground": "p:wight",
					"background": "p:red",
					"template": " 󰑮 {{ .FormattedMs }} ",
					"properties": {
						"threshold": 500,
						"style": "galveston"
					}
				},
				{
					"type": "go",
					"style": "powerline",
					"powerline_symbol": "",
					"foreground": "#FFF",
					"background": "#00ADD8",
					"template": "  {{ .Full }} "
				},
				{
					"type": "lua",
					"style": "powerline",
					"powerline_symbol": "",
					"foreground": "#FFF",
					"background": "#2C2D72",
					"template": "  {{ .Full }} "
				},
				{
					"type": "node",
					"style": "powerline",
					"powerline_symbol": "",
					"foreground": "#FFF",
					"background": "#5FA04E",
					"template": "  {{ .Full }} "
				},
				{
					"type": "rust",
					"style": "powerline",
					"powerline_symbol": "",
					"foreground": "#FFF",
					"background": "#000",
					"template": "  {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
				}
			]
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"properties": {
						"always_enabled": true
					},
					"style": "plain",
					"template": " 󰄾",
					"type": "status"
				}
			],
			"type": "prompt"
		}
	],
	"secondary_prompt": {
		"background": "transparent",
		"foreground": "#ffffff",
		"template": " 󰄾 "
	},
	"final_space": true,
	"version": 2
}
