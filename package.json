{
	"name": "stephansama",
	"author": "Stephan Randle <stephanrandle@hotmail.com>",
	"version": "1.0.0",
	"license": "MIT",
	"private": true,
	"repository": "https://github.com/stephansama/stephansama.git",
	"scripts": {
		"clean": "rm -rf node_modules/ && rm yarn.lock",
		"start": "yarn install && auto-changelog",
		"dev": "yarn collect",
		"test": "cd ./scripts/ && ./test.sh",
		"build": "cd ./scripts/ && ./install.sh",
		"collect": "cd ./scripts/ && ./collect.sh"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": true,
		"commitLimit": false
	},
	"devDependencies": {
		"auto-changelog": "*"
	}
}
